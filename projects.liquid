---
title: Projects
layout: layouts/main.liquid
---
<main id="projects" class="card">
  <h2 class="no-top-margin">Projects</h2>
  {%- for project in projects -%}
    <h3>
      <a target="_blank" rel="noopener noreferrer" href="{{ project.url }}">{{ project.title }}</a>
      {% if project.year %} ({{ project.year }}) {% endif %}
    </h3>
    <p>{{ project.description }}</p>
  {%- endfor -%}
</main>
